{
  "name": "natours",
  "version": "1.0.0",
  "description": "redo natours for learning css scss",
  "main": "index.js",
  "scripts": {
    "dev-server": "live-server --port=18452",
    "compile-and-watch-sass": "sass --watch --no-source-map sass/main.scss css/style.css",
    "start": "npm-run-all --parallel dev-server compile-and-watch-sass",
    "compile-sass": "sass --no-source-map sass/main.scss css/style.compile.css",
    "prefix-css": "postcss -u autoprefixer -b 'last 4 versions' --no-map css/style.compile.css -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuyeelok/advanced-css-course.git"
  },
  "author": "Kenneth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuyeelok/advanced-css-course/issues"
  },
  "homepage": "https://github.com/wuyeelok/advanced-css-course#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.2",
    "sass": "^1.43.4"
  }
}